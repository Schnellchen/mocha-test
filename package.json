{
  "name": "unit-testing-nodejs",
  "version": "1.0.0",
  "description": "unit testing project with mocha",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint '**/*.js'",
    "coverage": "nyc npm run test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "reporter": "html",
    "check-coverage": true,
    "branches": 45,
    "lines": 40,
    "functions": 40
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run coverage"
    }
  }
}
